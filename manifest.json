{
  "manifest_version": 3,
  "name": "TimeTrack Pro",
  "version": "1.1.0",
  "description": "Track time spent on websites and local files to analyze productivity",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/TIMETRACK PRO.png"
  },
  "icons": {
    "16": "icons/TIMETRACK PRO.png",
    "48": "icons/TIMETRACK PRO.png",
    "128": "icons/TIMETRACK PRO.png"
  },
  "options_page": "options.html"
}
